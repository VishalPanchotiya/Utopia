<form method="post" enctype="form-data" action="/token/write/transfer">

    <div class="form-group">
        <input type="text" class="form-control" placeholder="Enter address" id="mint_to" name="transfer_to"
            type="mint_to" data-rule-required="true" data-msg-required="Please enter address.">
    </div>
    <div class="form-group">
        <input type="text" class="form-control" placeholder="no_of_token_to_transfer" id="no_of_token_to_transfer"
            name="no_of_token_to_transfer" type="no_of_token_to_transfer" data-rule-required="true"
            data-msg-required="Please enter number.">
    </div>

    <div class="btn-gorup">
        <button type="submit" id="mint" class="btn btn-common">Transfer</button>
    </div>

</form>